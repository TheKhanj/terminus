{
  "name": "@nestjs/terminus",
  "version": "7.2.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@terminus)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp build && gulp move:protos",
    "build:all": "gulp build:all",
    "build:samples": "gulp install:samples && npm run build && gulp build:samples && gulp test:samples && gulp test:e2e:samples",
    "clean": "gulp clean",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json --detectOpenHandles",
    "test:ut:cov": "npm run test:ut -- --coverage",
    "release": "release-it"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "check-disk-space": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-angular": "12.1.4",
    "@grpc/grpc-js": "1.3.5",
    "@grpc/proto-loader": "0.6.4",
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "8.0.4",
    "@nestjs/core": "8.0.4",
    "@nestjs/microservices": "8.0.4",
    "@nestjs/mongoose": "8.0.0",
    "@nestjs/platform-express": "8.0.4",
    "@nestjs/platform-fastify": "8.0.4",
    "@nestjs/sequelize": "8.0.0",
    "@nestjs/swagger": "5.0.4",
    "@nestjs/testing": "8.0.4",
    "@nestjs/typeorm": "8.0.1",
    "@release-it/conventional-changelog": "3.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/mongoose": "5.11.97",
    "@types/node": "16.3.3",
    "@types/sequelize": "4.28.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "amqp-connection-manager": "3.2.2",
    "amqplib": "0.8.0",
    "cli-color": "2.0.0",
    "delete-empty": "3.0.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "fastify": "3.19.1",
    "fastify-formbody": "5.0.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-debug": "4.0.0",
    "gulp-typescript": "5.0.1",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "lint-staged": "11.0.1",
    "mongoose": "5.13.3",
    "mysql": "2.18.1",
    "mysql2": "2.2.5",
    "nats": "2.1.0",
    "portfinder": "1.0.28",
    "prettier": "2.3.2",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "release-it": "14.10.0",
    "rxjs": "7.2.0",
    "rxjs-compat": "6.6.7",
    "sequelize": "6.6.5",
    "sequelize-typescript": "2.1.0",
    "sort-package-json": "1.50.0",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.6",
    "ts-node": "10.1.0",
    "typeorm": "0.2.34",
    "typescript": "4.3.5",
    "wait-port": "^0.2.9"
  },
  "peerDependencies": {
    "@nestjs/common": "8.x",
    "@nestjs/core": "8.x",
    "reflect-metadata": "0.1.x",
    "rxjs": "7.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
